package com.SpringLifecycles;

public class Samosa {
	private int samosa;

	public int getSamosa() {
		return samosa;
	}

	public void setSamosa(int samosa) {
		this.samosa = samosa;
	}

	public Samosa(int samosa) {
		super();
		this.samosa = samosa;
	}

	@Override
	public String toString() {
		return "Samosa [samosa=" + samosa + "]";
	}

}
