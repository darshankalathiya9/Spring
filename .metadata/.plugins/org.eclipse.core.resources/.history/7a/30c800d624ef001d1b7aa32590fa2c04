package com.SpringLifecycles;

public class Dhosa {
	private int price;

	public int getPrice() {
		return price;
	}

	public void setPrice(int price) {
		this.price = price;
	}

	@Override
	public String toString() {
		return "Dhosa [price=" + price + "]";
	}

	public Dhosa() {
		super();
		// TODO Auto-generated constructor stub
	}

}
