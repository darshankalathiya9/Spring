package com.Constructor_Injection;

public class User {
	private int ID;
	private String Name;
	private double per;
	
	public User(int iD, String name, double per) {
		System.out.println("Set a Value by Defualt Constructor Arguments.");
		super();
		ID = iD;
		Name = name;
		this.per = per;
	}

	public User(String iD, String name, String per) {
		super();
		ID = iD;
		Name = name;
		this.per = per;
	}
	
	
}
